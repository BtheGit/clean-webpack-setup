{ 
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  rules:{
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "semi": 1,
    "object-curly-spacing": 1,
    "eol-last": 1,
    "no-trailing-spaces": 1
  } 
}